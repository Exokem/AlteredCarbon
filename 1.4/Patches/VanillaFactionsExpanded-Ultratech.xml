<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<!-- ================================ Patches in new stuff into the base game ====================== -->
		<operations>
			<!-- ================================ Patches in new mechanoids into the default faction pawnGroupMakers ====================== -->
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="VFEU_SleeveIncubator" or defName="VFEU_SleeveCasket" or defName="VFEU_CorticalStackStorage"]</xpath>
				<value>
					<designationCategory>Biotech</designationCategory>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="VFEU_SleeveIncubator"]</xpath>
				<value>
					<researchPrerequisites>
						<li>AC_SleeveProduction</li>
					</researchPrerequisites>
					<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
					<costList>
						<Steel>250</Steel>
						<Plasteel>50</Plasteel>
						<ComponentSpacer>3</ComponentSpacer>
					</costList>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="VFEU_SleeveCasket"]</xpath>
				<value>
					<researchPrerequisites>
						<li>AC_SleeveProduction</li>
					</researchPrerequisites>
					<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
					<costList>
						<Steel>180</Steel>
						<Plasteel>30</Plasteel>
						<ComponentSpacer>3</ComponentSpacer>
					</costList>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="VFEU_CorticalStackStorage"]</xpath>
				<value>
					<researchPrerequisites>
						<li>AC_CorticalStackMatrix</li>
					</researchPrerequisites>
					<constructionSkillPrerequisite>8</constructionSkillPrerequisite>
					<costList>
						<Steel>250</Steel>
						<Plasteel>150</Plasteel>
						<ComponentSpacer>12</ComponentSpacer>
					</costList>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>