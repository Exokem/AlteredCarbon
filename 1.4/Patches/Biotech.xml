<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<nomatch Class="PatchOperationSequence">
			<operations>
				<!-- ================================ Designationor if no Biotech is found ====================== -->
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<DesignationCategoryDef>
							<defName>Biotech</defName>
							<label>biotech</label>
							<order>12</order>
							<specialDesignatorClasses>
								<li>Designator_Cancel</li>
								<li>Designator_Deconstruct</li>
								<li>AlteredCarbon.Designator_ExtractStack</li>
							</specialDesignatorClasses>
							<preferredColumn>1</preferredColumn>
							<researchPrerequisites>
								<li>Electricity</li>
							</researchPrerequisites>
						</DesignationCategoryDef>
					</value>
				</li>
			</operations>
		</nomatch>
		<match Class="PatchOperationSequence">
			<operations>
				<!-- ================================ Adds the stack extractor if Biotech is found ====================== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/DesignationCategoryDef[defName="Biotech"]/specialDesignatorClasses</xpath>
					<value>
						<li>AlteredCarbon.Designator_ExtractStack</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>