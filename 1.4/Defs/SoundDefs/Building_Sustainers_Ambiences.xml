<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ================================ Gene Centrifuge ====================== -->
	<SoundDef>
		<defName>AC_GeneCentrifuge_Ambience_Start</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Electricity/GeothermalPlant/Start/GeothermalPlantStart_1a</clipPath>
					</li>
				</grains>
				<volumeRange>16~16</volumeRange>
				<distRange>0~50.27907</distRange>
				<sustainLoop>False</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>
	<SoundDef>
		<defName>AC_GeneCentrifuge_Ambience_Stop</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Electricity/GeothermalPlant/Stop/GeothermalPlantStop_1a</clipPath>
					</li>
				</grains>
				<volumeRange>16~16</volumeRange>
				<distRange>0~50.27907</distRange>
				<sustainLoop>False</sustainLoop>
			</li>
		</subSounds>
	</SoundDef>
	<SoundDef>
		<defName>AC_GeneCentrifuge_Ambience</defName>
		<sustain>True</sustain>
		<context>MapOnly</context>
		<priorityMode>PrioritizeNearest</priorityMode>
		<sustainStartSound>GeothermalPlant_Ambience_Start</sustainStartSound>
		<sustainStopSound>GeothermalPlant_Ambience_Stop</sustainStopSound>
		<subSounds>
			<li>
				<muteWhenPaused>True</muteWhenPaused>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Electricity/GeothermalPlant/Run/GeothermalPlantRun_Loop1a</clipPath>
					</li>
				</grains>
				<volumeRange>15~15</volumeRange>
				<pitchRange>1~1.015</pitchRange>
				<distRange>0~30</distRange>
			</li>
		</subSounds>
	</SoundDef>
</Defs>
