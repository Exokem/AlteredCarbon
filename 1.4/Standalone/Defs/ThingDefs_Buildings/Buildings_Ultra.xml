<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ================================ Sleeve Building Base ====================== -->
	<ThingDef Name="VFEU_SleeveBuildingBase" ParentName="BuildingBase" Abstract="True">
		<altitudeLayer>Building</altitudeLayer>
		<techLevel>Ultra</techLevel>
		<uiOrder>2100</uiOrder>
		<pathCost>42</pathCost>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<buildingTags>
				<li>Biotech</li>
			</buildingTags>
		</building>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
	</ThingDef>
	<!-- ================================ Sleeve Vat ====================== -->
	<ThingDef ParentName="VFEU_SleeveBuildingBase">
		<defName>VFEU_SleeveIncubator</defName>
		<label>sleeve incubator</label>
		<description>Text.todo</description>
		<thingClass>AlteredCarbon.Building_SleeveGrower</thingClass>
		<passability>Impassable</passability>
		<castEdgeShadows>true</castEdgeShadows>
		<graphicData>
			<texPath>Things/Building/Misc/SleeveGrower/SleeveGrowerBottom</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(7,7)</drawSize>
			<shaderType>CutoutComplex</shaderType>
			<shadowData>
				<volume>(2.55, 0.75, 1.7)</volume>
			</shadowData>
		</graphicData>
		<uiIconPath>Things/Building/Misc/SleeveGrower/SleeveGrowerUI_Icon</uiIconPath>
		<uiIconScale>1.2</uiIconScale>
		<descriptionHyperlinks>
			<ThingDef MayRequire="Ludeon.RimWorld.Biotech">GeneBank</ThingDef>
			<ThingDef MayRequire="Ludeon.RimWorld.Biotech">GeneProcessor</ThingDef>
		</descriptionHyperlinks>
		<tickerType>Normal</tickerType>
		<drawerType>RealtimeOnly</drawerType>
		<size>(3,3)</size>
		<hasInteractionCell>true</hasInteractionCell>
		<interactionCellOffset>(0,0,2)</interactionCellOffset>
		<defaultPlacingRot>South</defaultPlacingRot>
		<neverMultiSelect>true</neverMultiSelect>
		<statBases>
			<MarketValue>3580</MarketValue>
			<MaxHitPoints>400</MaxHitPoints>
			<Flammability>0.5</Flammability>
			<Mass>60</Mass>
		</statBases>
		<costList>
			<Steel>150</Steel>
			<Plasteel>50</Plasteel>
			<ComponentSpacer>6</ComponentSpacer>
		</costList>
		<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
		<fillPercent>0.6</fillPercent>
		<pathCost Inherit="False">70</pathCost>
		<canOverlapZones>false</canOverlapZones>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<blockWind>true</blockWind>
		<building>
			<destroySound>BuildingDestroyed_Metal_Big</destroySound>
		</building>
		<placeWorkers>
			<li>PlaceWorker_FuelingPort</li>
			<li MayRequire="Ludeon.RimWorld.Biotech">PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<comps>
			<li Class="AlteredCarbon.CompProperties_SleeveGrowerPostDraw">
				<overlayTexPath>Things/Building/Misc/SleeveGrower/SleeveGrowingVatTop</overlayTexPath>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<shortCircuitInRain>true</shortCircuitInRain>
				<basePowerConsumption>350</basePowerConsumption>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelLabel>Neutroamine</fuelLabel>
				<fuelGizmoLabel>Neutroamine</fuelGizmoLabel>
				<outOfFuelMessage>Cannot grow: Needs neutroamine</outOfFuelMessage>
				<consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
				<targetFuelLevelConfigurable>false</targetFuelLevelConfigurable>
				<fuelCapacity>300</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>Neutroamine</li>
					</thingDefs>
				</fuelFilter>
				<initialFuelPercent>0</initialFuelPercent>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
				<showFuelGizmo>true</showFuelGizmo>
			</li>
			<li MayRequire="Ludeon.RimWorld.Biotech" Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>GeneBank</li>
					<li>GeneProcessor</li>
				</linkableFacilities>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>5</glowRadius>
				<glowColor>(96,161,153,0)</glowColor>
			</li>
			<li Class="CompProperties_Flickable"/>
		</comps>
	</ThingDef>
	
